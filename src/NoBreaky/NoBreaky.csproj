<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<Version>0.0.1</Version>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<Title>NoBreaky</Title>
	<Description>Avoid Breaking Changes in ASP.NET Core REST APIs</Description>
	<PackageProjectUrl>https://github.com/thisisnabi/NoBreaky</PackageProjectUrl>
	<RepositoryUrl>https://github.com/thisisnabi/NoBreaky</RepositoryUrl>
	<PackageTags>asp-net-core; dotnet</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Testing" Version="8.0.8" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="xunit.assert" Version="2.6.6" />
  </ItemGroup>

</Project>
